#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
    int i = 2, n, T1 = 1, T2 = 1, T3;
    printf ("Quantos termos quer mostrar?: ");
    scanf("%d", &n);
    
    printf(" 1 - 1");
    while(i < n){
        T3 = T2 + T1;
        printf (" - %d", T3);
        T1 = T2;
        T2 = T3;
        i++;
    }
    printf ("\n");
	return 0;
    }
